{
  "compilerOptions":{
    "allowSyntheticDefaultImports": true,
    "declaration":true,
    "emitDecoratorMetadata":true,
    "esModuleInterop":true,
    "experimentalDecorators":true,
    "importHelpers":true,
    "module":"commonjs",
    "noImplicitReturns":true,
    "noUnusedLocals":true,
    "outDir":"lib",
    "resolveJsonModule":true,
    "sourceMap":true,
    "strict":true,
    "target":"es2019",
  },
  "include":["src"],
}